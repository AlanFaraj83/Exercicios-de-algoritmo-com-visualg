   algoritmo "controle de fluxo"
       var
         i: inteiro
         j: inteiro
   inicio
     i <- 1
     enquanto i <= 10 faca
       escreva (i:4)
       i <- i + 1
     fimenquanto
     
      escreval ("")
      j<- 1
      repita
         escreva (j:4)
         j<- j + 1
      ate j > 10
      
      
   fimalgoritmo
