algoritmo "Conversão time"


var
  hora:real
  minutos:real

inicio
     Escreva ("Digite a hora:")
     leia(hora)
     Escreva("Digite os minutos:")
     leia (minutos)
     Escreval("hora=",(60*minutos) )
     Escreval("horasminutos=", (minutos*60))
     Escreva("totalminutos=",(hora*3600))
fimalgoritmo
