algoritmo "variaveis"
   var
      idade: inteiro
      altura: real
      nome: caractere
      casado: logico
      vet_uni: vetor [0..4] de real
      vet_bi: vetor[0..1,0..3] de inteiro

inicio
   idade <- 30
   altura <- 1.82
   nome <- "Alan"
   casado <- verdadeiro
   vet_uni[2] <- 25
   vet_bi[1,3] <- 50
fimalgoritmo
