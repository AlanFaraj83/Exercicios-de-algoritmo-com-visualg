   algoritmo "controle de fluxo"
       var
         num1: inteiro
   inicio
      escreva (" Digite um número inteiro:")
      leia (num1)
      se ( num1 > 50) entao
             escreva ("Você digitou um número maior que 50")
      senao
             escreva ("Você digitou um número menor que 50")
      fimse
    fimalgoritmo
